<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>RozeComputing.Common.Models</RootNamespace>
    <TargetFramework>net6.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Rose Computing Common Models</Title>
    <Authors>Roze Computing, Matthew Rozendaal</Authors>
    <Description>Models used by and shared by Roze Computing Services</Description>
    <PackageTags>models</PackageTags>
    <PackageReleaseNotes>Added a model for DataTables. May move to a different namespace later on as things are added.</PackageReleaseNotes>
    <PackageOutputPath>D:\Programming\RozeComputing.Common.Cleaner</PackageOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
